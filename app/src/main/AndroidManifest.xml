<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.group2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconfr"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconfr"
        android:supportsRtl="true"
        android:theme="@style/Theme.Group2">
        <activity android:name=".MainActivity" />
        <activity android:name=".changeUsername"></activity>
        <activity android:name=".changePassword"></activity>
        <activity android:name=".ForgotPassword"></activity>
        <activity android:name=".Logout"></activity>
        <activity android:name=".viewProfile"></activity>
        <activity android:name=".PasswordUpdated" ></activity>
        <activity android:name=".NewCredentials"></activity>
        <activity android:name=".CodeVerify"></activity>
        <activity android:name=".Selection" ></activity>
        <activity android:name=".SignUp"></activity>
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>